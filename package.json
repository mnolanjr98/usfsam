{
  "name": "usf-sam",
  "version": "1.0.2",
  "author": "Mike Nolan <michael.nolan@usfoods.com>",
  "license": "MIT",
  "module": "dist/usf-sam.js",
  "main": "dist/usf-sam.umd.js",
  "types": "dist/usf-sam.d.ts",
  "scripts": {
    "clean": "rm -rf build && rm -rf dist && rm -rf packagenpm",
    "ngc": "ngc -p config/tsconfig.build.json",
    "rollup:fesm": "rollup build/usf-sam.js -o dist/usf-sam.js",
    "rollup:umd": "rollup build/usf-sam.js -o dist/usf-sam.umd.js --format umd --name usfSAM",
    "copy-metadata": "find build/* -not -name '*.js' -exec cp {} dist \\;",
    "build": "npm run clean && npm run ngc && npm run rollup:fesm && npm run rollup:umd && npm run copy-metadata",
    "test": "karma start config/karma.conf.js",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "publish-lib": "npm run test && npm run build",
    "pack-me-up": "npm pack"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/compiler-cli": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@types/jasmine": "2.5.41",
    "@types/node": "^6.0.45",
    "codelyzer": "~3.1.1",
    "core-js": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "^2.0.3",
    "marked": "^0.3.6",
    "phantomjs-prebuilt": "^2.1.7",
    "reflect-metadata": "0.1.3",
    "rollup": "^0.41.6",
    "rxjs": "^5.0.3",
    "ts-loader": "^1.0.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.2.2",
    "webpack": "2.3.3",
    "zone.js": "^0.8.5"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.2",
    "@angular/common": ">=4.0.2"
  }
}